{
  "kind": "collectionType",
  "collectionName": "resellers",
  "info": {
    "singularName": "reseller",
    "pluralName": "resellers",
    "displayName": "Reseller",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "link": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "regex": "^https:\\/\\/[^\\s\\/$.?#].[^\\s]*$",
      "required": true
    },
    "site": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "regex": "^https:\\/\\/[^\\s\\/$.?#].[^\\s]*$"
    },
    "email": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "email"
    },
    "country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    },
    "phone": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "city": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "address": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    }
  }
}
