{
  "kind": "collectionType",
  "collectionName": "coins",
  "info": {
    "singularName": "coin",
    "pluralName": "coins",
    "displayName": "Coin",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "coinId": {
      "type": "string"
    },
    "glowColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "faq": {
      "type": "json"
    },
    "coinName": {
      "type": "string"
    },
    "aboutCoin": {
      "type": "richtext"
    },
    "networks": {
      "type": "text"
    },
    "descriptionFAQ": {
      "type": "text"
    },
    "slug": {
      "type": "string"
    },
    "logo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "website": {
      "type": "string"
    }
  }
}
